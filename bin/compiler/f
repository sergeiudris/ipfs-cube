#!/bin/bash

shadow(){
    ./node_modules/.bin/shadow-cljs "$@"
}

dev(){
  npm i
  shadow -A:shadow:core:ui:ipfs-link watch :ui :ipfs-link
}

server(){
    shadow -A:shadow:core:ui:ipfs-link server
    # yarn server
}

compile(){
    npm i
    shadow -A:shadow:core:ui:ipfs-link compile :ui :ipfs-link
}

release(){
    npm i
    shadow -A:shadow:core:ui:ipfs-link release :ui :ipfs-link
}


"$@"