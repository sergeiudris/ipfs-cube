#!/bin/bash

repl(){
    clj -A:core:app:repl
}


main(){
  clojure -A:core:app
}


uberjar(){
  clojure -X:depstar uberjar \
    :aot true \
    :jar target/find.jar \
    :aliases '[:core :app]' \
    :main-class find.app.main
}

resources(){
  mkdir -p resources/repo/
  cp -r ../../src resources/repo/
}

"$@"